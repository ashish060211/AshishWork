package executors;

public class ThreadLocalExample1 {
	
	ThreadLocal<Example> tl = null;
	ThreadLocalExample1 () {
		tl = new ThreadLocal<Example>();
	}

	public static void main(String[] args) {


	}
	
	void beforCommit() {
		Example ex = new Example(1,"ashish");
		tl.set(ex);
		System.out.println("before commit end...");
	}
	
	class Example {
		int id;
		String name;
		public Example(int id, String name) {
			super();
			this.id = id;
			this.name = name;
		}
		public int getId() {
			return id;
		}
		public void setId(int id) {
			this.id = id;
		}
		public String getName() {
			return name;
		}
		public void setName(String name) {
			this.name = name;
		}
	}

}
