package com.ash.java.geekforgeek.code;

import java.util.Arrays;
import java.util.Scanner;

public class FindSubArraySumProblem {

	public static void main(String[] args) {
		
		 Scanner sc = new Scanner(System.in);
		 int tcNum = sc.nextInt();
		 
		 for(int i=0;i<tcNum;i++) {
			 ArrayInfo arrayInfo = getArrayInfo(Scanner sc); 
			 System.out.println(arrayInfo);
		 }
		 
	}

	static class ArrayInfo {
		int size;
		int sum;
		int[] array;
		
		public ArrayInfo(int size, int sum, int[] array) {
			super();
			this.size = size;
			this.sum = sum;
			this.array = array;
		}
		public int getSize() {
			return size;
		}
		public void setSize(int size) {
			this.size = size;
		}
		public int getSum() {
			return sum;
		}
		public void setSum(int sum) {
			this.sum = sum;
		}
		public int[] getArray() {
			return array;
		}
		public void setArray(int[] array) {
			this.array = array;
		}
		@Override
		public String toString() {
			return "ArrayInfo [size=" + size + ", sum=" + sum + ", array=" + Arrays.toString(array) + "]";
		}
	}
	
	static int[] convertStringIntoArray(String str, int size) {
		int[] arr = new int[size];
		String[] strarr = str.split(" ");
		for(int i =0;i<size;i++) {
			int j = Integer.valueOf(strarr[i]);
			arr[i] = j;
		}
		System.out.println(arr);
		return arr;
	}
	
	static ArrayInfo getArrayInfo() {
		Scanner sc = new Scanner(System.in);
		String line1 = sc.nextLine();
		int size = Integer.valueOf(line1.split(" ")[0]);
		int sum = Integer.valueOf(line1.split(" ")[1]);
		String line2 = sc.nextLine();
		int[] array = convertStringIntoArray(line2, size);
		sc.close();
		return new ArrayInfo(size, sum, array);
	}
}
