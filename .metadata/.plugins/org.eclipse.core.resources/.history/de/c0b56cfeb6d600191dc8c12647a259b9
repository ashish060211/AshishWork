package executors;

import java.util.UUID;

public class ThreadLocalExample1 {
	
	ThreadLocal<Example> tl = null;
	ThreadLocalExample1 () {
		tl = new ThreadLocal<Example>();
	}

	public static void main(String[] args) {


	}
	
	void beforCommit() {
		Example ex = new Example(UUID.randomUUID().toString());
		tl.set(ex);
		System.out.println("before commit end..."+ex);
	}
	void afterCommit() {
		System.out.println("after commit end..."+tl.get());
	}
	
	class Example {
		String name;
		public Example(String name) {
			super();
			this.name = name;
		}
		public int getId() {
			return id;
		}
		public void setId(int id) {
			this.id = id;
		}
		public String getName() {
			return name;
		}
		public void setName(String name) {
			this.name = name;
		}
		@Override
		public String toString() {
			return "Example [id=" + id + ", name=" + name + "]";
		}
	}

}
